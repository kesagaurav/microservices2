package com.gaurav.dto;

import com.gaurav.model.Employee;

import lombok.Data;

@Data
public class EmployeeDTO {
	private int empid;
	private String name;
	private String desc;
	
	private Employee dtoToEmployee(	EmployeeDTO  empDTO) {
		Employee e=new Employee();
		e.setEmpid(empDTO.getEmpid());
		e.setName(empDTO.getName());
		e.setDesc(empDTO.getDesc());
		return e;
	}
}
